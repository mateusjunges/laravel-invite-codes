{
    "name": "mateusjunges/laravel-invite-codes",
    "description": "This package allows you to easily manage invite codes for your Laravel application.",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mateus Junges",
            "email": "mateus@junges.dev",
            "homepage": "https://twitter.com/mateusjungess",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.4",
        "illuminate/auth": "^11.45|^12.0",
        "illuminate/support": "^11.45|^12.0",
        "illuminate/database": "^11.45|^12.0",
        "laravel/pint": "dev-main"
    },
    "require-dev": {
        "orchestra/testbench": "8.0|^10.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0",
        "predis/predis": "^1.1|^2.3",
        "rector/rector": "dev-main"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Junges\\InviteCodes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Junges\\InviteCodes\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Junges\\InviteCodes\\InviteCodesServiceProvider",
                "Junges\\InviteCodes\\InviteCodesEventServiceProvider"
            ]
        }
    },
    "scripts": {
        "pest": "vendor/bin/pest",
        "test": "@pest --parallel",
        "format": "vendor/bin/pint",
        "rector": "vendor/bin/rector process src"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
